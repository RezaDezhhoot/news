                                <h2 class="text-dark font-weight-bold my-1 mr-5"><%= page_title %></h2>
                            </div>
                        </div>
                        <% if (has_action) { %>
                            <div class="d-flex align-items-center">
                                <a href="<%= action_url %>" class="btn btn-light-primary font-weight-bolder btn-sm"><%= action_title %></a>
                            </div>
                        <% } %>
                    </div>
                </div>
                <div class="d-flex flex-column-fluid">
