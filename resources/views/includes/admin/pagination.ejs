<nav>
    <ul class="pagination justify-content-center">
        <% if(currentPage !== 1 && prePage !== 1) { %>
            <li class="page-item"><a class="page-link" href="?page=1<%=search ? '&search='+search : '' %>">1</a></li>
        <% } %>
            <% if(hasPrePage) { %>
            <li class="page-item"><a class="page-link" href="?page=<%=prePage%><%=search ? '&search='+search : '' %>"><%=prePage%></a></li>
            <% } %>
        <li class="page-item active"><a class="page-link" href="?page=<%=currentPage%><%=search ? '&search='+search : '' %>"><%=currentPage%></a></li>
            <% if(hasNextPage) { %>
            <li class="page-item"><a class="page-link" href="?page=<%=nextPage%><%=search ? '&search='+search : '' %>"><%=nextPage%></a></li>
            <% } %>
            <% if(lastPage !== currentPage && nextPage !== lastPage) { %>
            <li class="page-item"><a class="page-link">...</a></li>
            <li class="page-item"><a class="page-link" href="?page=<%=lastPage%><%=search ? '&search='+search : '' %>"><%=lastPage%></a></li>
            <% } %>
    </ul>
</nav>